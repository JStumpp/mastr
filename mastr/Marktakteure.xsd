        <xs:element type="xs:boolean" name="Registergericht_nv" minOccurs="0"/> <!-- Mandatory according to [1] but not always present -->
        <xs:element type="xs:string" name="RegistergerichtAusland" minOccurs="0"/>
        <xs:element type="xs:boolean" name="RegistergerichtAusland_nv" minOccurs="0"/>
        <xs:element type="xs:string" name="Registernummer" minOccurs="0"/> <!-- Mandatory according to [1] but not always present -->
