        <xs:element type="xs:boolean" name="BelieferungVonLetztverbrauchernStrom" minOccurs="0"/>
        <xs:element type="xs:boolean" name="BelieferungHaushaltskundenStrom" minOccurs="0"/>
        <xs:element type="xs:boolean" name="Gasgrosshaendler" minOccurs="0"/>
        <xs:element type="xs:boolean" name="Stromgrosshaendler" minOccurs="0"/>
        <xs:element type="xs:boolean" name="BelieferungVonLetztverbrauchernGas" minOccurs="0"/>
        <xs:element type="xs:boolean" name="BelieferungHaushaltskundenGas" minOccurs="0"/>
      </xs:all>
