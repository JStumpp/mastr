        <xs:element type="xs:nonNegativeInteger" name="LandAnZustelladresse" minOccurs="0"/>
        <xs:element type="xs:string" name="PostleitzahlAnZustelladresse" minOccurs="0"/>
        <xs:element type="xs:string" name="OrtAnZustelladresse" minOccurs="0"/>
        <xs:element type="xs:string" name="StrasseAnZustelladresse" minOccurs="0"/>
        <xs:element type="xs:string" name="HausnummerAnZustelladresse" minOccurs="0"/>
        <xs:element type="xs:boolean" name="HausnummerAnZustelladresse_nv" minOccurs="0"/>
        <xs:element type="xs:string" name="AdresszusatzAnZustelladresse" minOccurs="0"/>
        <xs:element type="xs:boolean" name="Kmu" minOccurs="0"/>
        <xs:element type="xs:string" name="TelefonnummerVMav" minOccurs="0"/>
        <xs:element type="xs:string" name="EmailVMav" minOccurs="0"/>
        <xs:element type="xs:dateTime" name="RegistrierungsdatumMarktakteur" minOccurs="0"/>
        <xs:element type="xs:nonNegativeInteger" name="HauptwirtdschaftszweigAbteilung" minOccurs="0"/>
