        <xs:element type="xs:nonNegativeInteger" name="Land" minOccurs="0"/>
        <xs:element type="xs:string" name="Region" minOccurs="0"/>
        <xs:element type="xs:string" name="Strasse" minOccurs="0"/>
        <xs:element type="xs:string" name="Hausnummer" minOccurs="0"/> <!-- Mandatory according to [1] but not always present -->
        <xs:element type="xs:boolean" name="Hausnummer_nv" minOccurs="0"/> <!-- Mandatory according to [1] but not always present -->
        <xs:element type="xs:string" name="Adresszusatz" minOccurs="0"/>
        <xs:element type="xs:string" name="Postleitzahl" minOccurs="0"/>
        <xs:element type="xs:string" name="Ort" minOccurs="0"/>
        <xs:element type="xs:nonNegativeInteger" name="Bundesland" minOccurs="0"/>
        <xs:element type="xs:string" name="Netz" minOccurs="0"/>
        <xs:element type="xs:string" name="Nuts2" minOccurs="0"/>
