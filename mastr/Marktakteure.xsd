<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Marktakteure">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Marktakteur" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Marktakteur">
    <!--
      Created using [1] as a guide. Discrepancies are marked with comments.
      [1] https://web.archive.org/web/20210918184738/https://www.marktstammdatenregister.de/MaStRHilfe/files/gesamtdatenexport/Dokumentation%20MaStR%20Gesamtdatenexport.pdf
    -->
    <xs:complexType>
      <xs:all>
        <!-- <xs:element type="xs:nonNegativeInteger" name="Id" minOccurs="0"/> -->
        <xs:element type="xs:string" name="MastrNummer"/>
