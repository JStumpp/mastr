        <xs:element type="xs:string" name="Telefon" minOccurs="0"/> <!-- Mandatory according to [1] but not always present -->
        <xs:element type="xs:string" name="Fax" minOccurs="0"/> <!-- Mandatory according to [1] but not always present -->
