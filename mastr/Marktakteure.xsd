        <xs:element type="xs:string" name="RegisternummerAusland" minOccurs="0"/>
        <xs:element type="xs:boolean" name="RegisternummerAusland_nv" minOccurs="0"/>
        <xs:element type="xs:date" name="Taetigkeitsbeginn" minOccurs="0"/>
